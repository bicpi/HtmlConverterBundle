{
    "name":        "bicpi/html2text-bundle",
    "type":        "symfony-bundle",
    "description": "This bundle provides Symfony2 integration of bicpi's Html2Text component",
    "keywords":    ["html2text", "bicpi"],
    "homepage":    "http://github.com/bicpi/Html2TextBundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Philipp Rieber",
            "homepage": "http://philipp-rieber.net"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "bicpi/bicpi-components": "1.*@dev"
    },
    "require-dev": {
        "symfony/dependency-injection": "~2.1",
        "symfony/http-kernel": "~2.1",
        "symfony/config": "~2.1",
        "symfony/yaml": "~2.1"
    },
    "autoload": {
        "psr-0": {
            "bicpi\\Bundle\\Html2TextBundle": ""
        }
    },
    "target-dir": "bicpi/Bundle/Html2TextBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
