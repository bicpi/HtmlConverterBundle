<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="bicpi.html_converter.guesser.class">bicpi\Component\HtmlConverter\Converter\ChainConverter</parameter>
        <parameter key="bicpi.html_converter.lynx.class">bicpi\Component\HtmlConverter\Converter\LynxConverter</parameter>
        <parameter key="bicpi.html_converter.html2text.class">bicpi\Component\HtmlConverter\Converter\HtmlConverterConverter</parameter>
        <parameter key="bicpi.html_converter.simple.class">bicpi\Component\HtmlConverter\Converter\SimpleConverter</parameter>
    </parameters>

    <services>
        <service id="bicpi.html_converter.guesser" class="%bicpi.html_converter.guesser.class%"/>
        <service id="bicpi.html_converter.lynx" class="%bicpi.html_converter.lynx.class%"/>
        <service id="bicpi.html_converter.html2text" class="%bicpi.html_converter.html2text.class%"/>
        <service id="bicpi.html_converter.simple" class="%bicpi.html_converter.simple.class%"/>
    </services>
</container>
