<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="bicpi.html2text.converter.chain.class">bicpi\Component\Html2Text\Converter\ChainConverter</parameter>
        <parameter key="bicpi.html2text.converter.lynx.class">bicpi\Component\Html2Text\Converter\LynxConverter</parameter>
        <parameter key="bicpi.html2text.converter.html2text.class">bicpi\Component\Html2Text\Converter\Html2TextConverter</parameter>
        <parameter key="bicpi.html2text.converter.simple.class">bicpi\Component\Html2Text\Converter\SimpleConverter</parameter>
    </parameters>

    <services>
        <service id="bicpi.html2text.converter.chain" class="%bicpi.html2text.converter.chain.class%"/>
        <service id="bicpi.html2text.converter.lynx" class="%bicpi.html2text.converter.lynx.class%"/>
        <service id="bicpi.html2text.converter.html2text" class="%bicpi.html2text.converter.html2text.class%"/>
        <service id="bicpi.html2text.converter.simple" class="%bicpi.html2text.converter.simple.class%"/>
    </services>
</container>
